@model IEnumerable<Superhero.Models.Superheroes>

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.alterEgo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.superHeroAbility)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.secondSuperHeroAbility)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.catchphrase)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.alterEgo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.superHeroAbility)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.secondSuperHeroAbility)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.catchphrase)
            </td>

        </tr>
    }
    <div>
        @Html.ActionLink("Back to List", "Index")
    </div>

</table>
